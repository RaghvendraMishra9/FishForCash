                        {
                            <div class="col col-sm-2 nopadding">
                                <div class="booty-tile">
                                    <div class="actions-tile">
                                        <div class="circle-inner">
                                            <img src="@Url.Content(String.Format("~/BOOTY_ICONS/{0}.gif", "r"))" class="img-fluid  rounded-circle">
                                        </div>
                                    </div>
                                    <div class="item-count">
                                        @Model.r
                                    </div>
                                    <a  asp-action="BootyMarket" asp-route-tradebooty="r" class="btn btn-warning">Check</a>
                                </div>
                            </div>
                        }
                    </div>
                   
                </div>
            </div>
        </div>
    </main>
</section>

