                                <div class="card">
                                    <table class="table table-striped table-lg">
                                        <tbody>
                                            @foreach (var item in ViewBag.CashPrizeCount)
                                            {
                                                <tr>
                                                    <td>@item.PrizeType</td>
                                                    <td class="text-right"><strong>@item.BalanceCount</strong></td>
                                                </tr>

                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>


