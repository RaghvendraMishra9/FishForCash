                                <h3 class="title-2 m-b-25">Support Center</h3>
                                <div class="card">

                                    <table border="0" cellpadding="0" class="table">

                                        <tbody>
                                            <tr>
                                                <td class="align-middle">
                                                    <h4> Support Emails  <span class="badge badge-success">5 UNREAD</span></h4>
                                                </td>
                                                <td>
                                                    <a href="#" class="btn btn-secondary btn-block">View</a>
                                                </td>


                                            </tr>
                                            <tr></tr>
                                            <tr>
                                                <td class="align-middle">
                                                    <h4>  Smudged Gamecodes  <span class="badge badge-info">@ViewBag.SmudgedCount</span></h4>
                                                </td>
                                                <td>
                                                    <a asp-action="SmudgedCode" asp-controller="Admin" class="btn btn-secondary btn-block">View</a>
                                                </td>


                                            </tr>
                                            <tr>

                                                <td class="align-middle">
                                                    <h4> Already Played Errors <span class="badge badge-info">@ViewBag.AlreadyPlayedCount</span></h4>
                                                </td>
                                                <td>
                                                    <a asp-action="AlreadyplayedCode" asp-controller="Admin" class="btn btn-secondary btn-block">View</a>
                                                </td>

                                            </tr>
                                            <tr>

                                                <td class="align-middle">
                                                    <h4> Free Samples<span class="badge badge-info">4</span></h4>
                                                </td>
                                                <td>
                                                    <a href="#" class="btn btn-secondary btn-block">View</a>
                                                </td>

                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <h3 class="title-2 m-b-25">Players Registered</h3>
                                <div id="chart-container"></div>
                                <div class="card">

                                    <table border="0" cellpadding="0" class="table table-striped">

                                        <tbody>
                                            <tr>
                                                <td class="align-middle">
                                                    This Week
                                                </td>
                                                <td class="text-right">
                                                    <strong>95</strong>
                                                </td>


                                            </tr>
                                            <tr>
                                                <td class="align-middle">
                                                    Last Week
                                                </td>
                                                <td class="text-right">
                                                    <strong>235</strong>
                                                </td>

                                            </tr>
                                            <tr>
                                                <td class="align-middle">
